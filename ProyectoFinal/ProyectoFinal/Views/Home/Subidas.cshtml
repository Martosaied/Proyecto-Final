@using ProyectoFinal.Models
@{
    ViewBag.Title = "Mis Subidas";
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Mis Subidas</title>
    <link href='http://fonts.googleapis.com/css?family=Nunito:400,300' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="css/main.css">
</head>
<body>
    <div>
        <div class="row" style="margin:1%;">
            @{
                foreach (Contenido item in ViewBag.ListaArticulos)
                {
                    <div class="col-sm-6 col-md-4">
                        <div class="thumbnail">
                            <div class="caption">
                                <h3>@item.Nombre</h3>
                                <p>@item.Descripcion</p>
                                <h4>@item.IdEscuelas</h4>
                                <h4>@item.IdMateria</h4>
                                <h4>@item.Profesor</h4>
                                <p><a href=@item.Ruta class="btn btn-primary" role="button">Button</a> <a href="#" class="btn btn-default" role="button">Button</a></p>
                            </div>
                        </div>
                    </div>

                }
            }
        </div>
    </div>
</body>
</html>
